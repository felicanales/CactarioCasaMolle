# Versión alternativa más simple
[providers]
node = "20"
python = "3.11"

[phases.install]
cmds = [
    "npm install",
    "cd nextjs && npm install",
    "cd fastapi && pip install -r requirements.txt"
]

[phases.build]
cmds = [
    "cd nextjs && npm run build"
]

[start]
cmd = "npm run start:all"
